    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.8"
  }
}
